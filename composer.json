{
    "name": "augustpermana/laravel-meta-generator",
    "description": "A Laravel package to generate and manage metadata for models",
    "minimum-stability": "dev",
    "prefer-stable": true,
    "type": "laravel-package",
    "license": "MIT",
    "keywords": [
        "laravel",
        "meta",
        "meta-table",
        "meta-generator",
        "meta-table-generator"
    ],
    "authors": [
        {
            "name": "Agus Permana",
            "email": "agus.emailnya@gmail.com"
        }
    ],
    "homepage": "https://github.com/agus-gian/laravel-meta-generator",
    "require": {
        "php": ">=7.3",
        "laravel/framework": ">=8.0",
        "illuminate/support": "^8.0|^9.0|^10.0|^11.0",
        "illuminate/database": "^8.0|^9.0|^10.0|^11.0",
        "illuminate/events": "^8.0|^9.0|^10.0|^11.0",
        "ext-json": "*"
    },
    "autoload": {
        "psr-4": {
            "AugustPermana\\MetaGenerator\\": "src/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-main": "1.x-dev",
            "dev-master": "1.x-dev"
        },
        "laravel": {
            "providers": [
                "AugustPermana\\MetaGenerator\\ServiceProvider"
            ]
        }
    }
}
